<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.baidu.ocr.user">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.baidu.ocr.user.BaiduMainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.baidu.ocr.user.IDCardActivity"
            android:label="@string/idCardLabel">
        </activity>
        <activity android:name="com.baidu.ocr.user.BankCardActivity"
            android:label="@string/bankCardLabel">
        </activity>
        <activity android:name="com.baidu.ocr.user.GeneralActivity"
            android:label="@string/generalLabel">
        </activity>
    </application>

</manifest>